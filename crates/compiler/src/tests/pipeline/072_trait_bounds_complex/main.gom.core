fn combine_and_render(a/21: T, b/22: T) -> string {
  let scaled/23 = trait_call[Scale::scale](trait_call[Add::add](a/21, b/22), 2) in
  let same/24 = trait_call[Eq::eq](a/21, b/22) in
  let sum_hash/25 = (trait_call[Hash::hash](a/21) + trait_call[Hash::hash](b/22)) in
  let dbg/26 = trait_call[Debug::show](scaled/23) in
  let disp/27 = trait_call[Display::show](scaled/23) in
  ((((dbg/26 + " | ") + disp/27) + " | eq=") + (bool_text(same/24) + (" | hash=" + int32_to_string(sum_hash/25))))
}

fn sum_and_inspect(x/28: T, y/29: T, z/30: T) -> string {
  let total/31 = trait_call[Add::add](trait_call[Add::add](x/28, y/29), z/30) in
  let h/32 = trait_call[Hash::hash](total/31) in
  ((trait_call[Inspect::inspect](total/31) + " @") + int32_to_string(h/32))
}

