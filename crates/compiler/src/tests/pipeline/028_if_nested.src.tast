fn classify(x/0: int) -> string {
  if (int_less : (int, int) -> bool)((x/0 : int), 0) {
      "negative"
  } else {
      if (int_less : (int, int) -> bool)(0, (x/0 : int)) {
          "positive"
      } else {
          "zero"
      }
  }
}

fn triangle_type(a/1: int, b/2: int, c/3: int) -> string {
  if (int_less : (int, int) -> bool)((a/1 : int), (b/2 : int)) {
      if (int_less : (int, int) -> bool)((b/2 : int), (c/3 : int)) {
          "ascending"
      } else {
          "peak"
      }
  } else {
      if (int_less : (int, int) -> bool)((a/1 : int), (c/3 : int)) {
          "valley"
      } else {
          "flat"
      }
  }
}

fn main() -> unit {
  let first/4: string = (classify : (int) -> string)(-42) in
  let second/5: string = (classify : (int) -> string)(0) in
  let third/6: string = (classify : (int) -> string)(17) in
  let shape1/7: string = (triangle_type : (int, int, int) -> string)(1, 2, 3) in
