fn make_point(base/0: int32) -> Point {
  Point { x: base/0, y: (base/0 + 1) }
}

fn sum_point(p/1: Point) -> int32 {
  (Point.x(p/1) + Point.y(p/1))
}

fn main() -> int32 {
  let p/2 = make_point(5) in
  let shifted/3 = Point { x: (Point.x(p/2) + 1), y: (Point.y(p/2) - 2) } in
  (Point.x(shifted/3) + sum_point(shifted/3))
}