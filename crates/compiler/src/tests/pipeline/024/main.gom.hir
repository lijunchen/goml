struct Point { x: int32, y: int32 }

fn point32_to_string(local_0: Point) -> string {
  let Point { x: local_1, y: local_2 } = local_0 in
  "Point { x: " + int32_to_string(local_1) + ", y: " + int32_to_string(local_2) + " }"
}

enum Color { Red, Green, Blue }

fn color_to_string(local_0: Color) -> string {
  match local_0 {
    Color::Red => "Red",
    Color::Green => "Green",
    Color::Blue => "Blue",
  }
}

struct Line { from: Point, to: Point, color: Color }

fn line_to_string(local_0: Line) -> string {
  let Line { from: local_1, to: local_2, color: local_3 } = local_0 in
  "Line { from: " + point32_to_string(local_1) + ", to: " + point32_to_string(local_2) + ", color: " + color_to_string(local_3) + " }"
}

fn point_type(local_0: Point) -> string {
  match local_0 {
    Point { x: 0, y: 0 } => "origin",
    Point { x: 0, y: 1 } => "up",
    Point { x: 1, y: 0 } => "right",
    Point { x: 0, y: local_1 } => match 0 < local_1 {
      true => "above",
      false => "below",
    },
    _ => "unknown",
  }
}

fn main() -> () {
  let local_0 = Point { x: 0, y: 0 } in
  let _ = string_println(point_type(local_0)) in
  let local_1 = Point { x: 10, y: 10 } in
  let local_2 = Line { from: local_0, to: local_1, color: Color::Red } in
  string_println(line_to_string(local_2))
}