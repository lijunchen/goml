fn choose(flag: bool, x: int32, y: int32) -> int32 {
    if flag {
        x
    } else {
        y
    }
}

fn main() {
    let yes = choose(true, 10, 99) in
    let no = choose(false, 10, 99) in
    let _ = string_println(string_add("yes=", int_to_string(yes))) in
    let _ = string_println(string_add("no=", int_to_string(no))) in
    ()
}

