fn my_int_equal(x/0: int32, y/1: int32) -> bool {
  bool_and(bool_not(int_less(x/0, y/1)), bool_not(int_less(y/1, x/0)))
}

fn sum(n/2: int32) -> int32 {
  if my_int_equal(n/2, 1) {
      1
  } else {
      int_add(n/2, sum(int_sub(n/2, 1)))
  }
}

fn main() -> unit {
  string_println(int_to_string(sum(100)))
}