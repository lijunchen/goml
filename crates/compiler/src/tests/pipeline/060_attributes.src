#[derive(ToString)]
enum Color {
  Red,
  Green,
  Blue,
}

#[wasm_bindgen]
struct Point {
  x: int32,
  y: int32,
}

#[inline(always)]
fn sum(point: Point) -> int32 {
  point.x + point.y
}

#[cfg(test)]
impl Color {
  fn identity(color: Color) -> Color {
    color
  }
}

#[link("math")]
extern "go" "math" abs(value: float64) -> float64

fn main() -> unit {
  let point = Point { x: 1, y: 2 };
  let _ = sum(point);
  string_println("()")
}
