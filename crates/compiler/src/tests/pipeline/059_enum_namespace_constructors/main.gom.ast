enum Color {
    Red,
    Green
}

enum Signal {
    Red,
    Yellow
}

fn color_is_red(color: Color) -> bool {
    match color {
        Color::Red => true,
        Color::Green => false,
    }
}

fn toggle_signal(signal: Signal) -> Signal {
    match signal {
        Signal::Red => Signal::Yellow,
        Signal::Yellow => Signal::Red,
    }
}

fn main() -> Signal {
    let current = Color::Red in
    let _ = color_is_red(current) in
    toggle_signal(Signal::Red)
}

