fn test_basic_sequence() {
    let _ = string_print("first") in
    let _ = string_print("second") in
    let _ = string_print("third") in
    ()
}

fn test_mixed_expressions() -> int {
    let _ = string_print("computing") in
    let x = 5 in
    let _ = string_print(int_to_string(x)) in
    x + 10
}

fn main() {
    let _ = test_basic_sequence() in
    let result = test_mixed_expressions() in
    let _ = string_print("result: ") in
    let _ = string_print(int_to_string(result)) in
    ()
}

