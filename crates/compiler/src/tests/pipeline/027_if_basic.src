fn choose(flag: bool, x: int32, y: int32) -> int32 {
  if flag {
    x
  } else {
    y
  }
}

fn main() {
  let yes = choose(true, 10, 99);
  let no = choose(false, 10, 99);
  let _ = string_println(string_add("yes=", int32_to_string(yes)));
  let _ = string_println(string_add("no=", int32_to_string(no)));
  ()
}
