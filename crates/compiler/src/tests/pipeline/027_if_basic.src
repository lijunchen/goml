fn choose(flag: bool, x: int, y: int) -> int {
  if flag {
    x
  } else {
    y
  }
}

fn main() {
  let yes = choose(true, 10, 99);
  let no = choose(false, 10, 99);
  let _ = string_println(string_add("yes=", int_to_string(yes)));
  let _ = string_println(string_add("no=", int_to_string(no)));
  ()
}
