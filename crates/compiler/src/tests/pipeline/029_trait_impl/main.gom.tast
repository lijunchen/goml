impl Display for Point{
  fn show(self/0: Point) -> string {
      "Point";
  }
}

impl Display for Maybe[int32]{
  fn show(self/1: Maybe[int32]) -> string {
      match (self/1 : Maybe[int32]) {
          Maybe::Just(value/2: int32) => "Just(" + (int32_to_string : (int32) -> string)((value/2 : int32)) + ")",
          Maybe::Nothing => "Nothing",
      };
  }
}

fn make_maybe(flag/3: bool) -> Maybe[int32] {
    if (flag/3 : bool) {
        {
            Maybe::Just(42);
        }
    } else {
        {
            Maybe::Nothing;
        }
    };
}

fn main() -> unit {
    let point/4: Point = Point { x: 1, y: 2 };
    let some_number/5: Maybe[int32] = (make_maybe : (bool) -> Maybe[int32])(true);
    let none_number/6: Maybe[int32] = (make_maybe : (bool) -> Maybe[int32])(false);
    let _ : unit = (string_println : (string) -> unit)((Display::show : (Self) -> string)((point/4 : Point)));
    let _ : unit = (string_println : (string) -> unit)((Display::show : (Self) -> string)((some_number/5 : Maybe[int32])));
    let _ : unit = (string_println : (string) -> unit)((Display::show : (Self) -> string)((none_number/6 : Maybe[int32])));
    ();
}