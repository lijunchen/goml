fn make_point(base/0: int32) -> Point {
  Point { x: (base/0 : int32), y: (base/0 : int32) + 1 }
}

fn sum_point(p/1: Point) -> int32 {
  (p/1 : Point).x + (p/1 : Point).y
}

fn main() -> int32 {
  let p/2: Point = (make_point : (int32) -> Point)(5) in
  let shifted/3: Point = Point { x: (p/2 : Point).x + 1, y: (p/2 : Point).y - 2 } in
  (shifted/3 : Point).x + (sum_point : (Point) -> int32)((shifted/3 : Point))
}