fn my_int_equal(x/0: int32, y/1: int32) -> bool {
  let t1 = int32_less(x/0, y/1) in
  let t0 = bool_not(t1) in
  let t3 = int32_less(y/1, x/0) in
  let t2 = bool_not(t3) in
  (t0 && t2)
}

fn sum(n/2: int32) -> int32 {
  let t4 = my_int_equal(n/2, 1) in
  if t4 {
    1
  } else {
    let t6 = (n/2 - 1) in
    let t5 = sum(t6) in
    (n/2 + t5)
  }
}

fn main() -> unit {
  let t8 = sum(100) in
  let t7 = int32_to_string(t8) in
  string_println(t7)
}