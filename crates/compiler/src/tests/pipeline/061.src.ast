struct Point {
    x: int32,
    y: int32,
}

impl Point {
  fn new(x: int32, y: int32) -> Point {
      Point {
          x: x,
          y: y
      }
  }
}

fn main() {
    let p = Point::new(10, 20) in
    let _ = string_println(int32_to_string(p.x)) in
    let _ = string_println(int32_to_string(p.y)) in
    ()
}

