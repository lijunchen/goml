#[derive(ToString)]
struct Point {
    x: int32,
    y: int32,
}

impl Point {
  fn to_string(self: Point) -> string {
      let Point { x: x, y: y } = self in
      string_add(string_add(string_add(string_add(string_add(string_add("Point { ", "x: "), x.to_string()), ", "), "y: "), y.to_string()), " }")
  }
}

impl Point {
  fn new(x: int32, y: int32) -> Point {
      Point {
          x: x,
          y: y
      }
  }
}

fn main() {
    let p = Point::new(10, 20) in
    let _ = string_println(p.to_string()) in
    ()
}

