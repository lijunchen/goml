fn choose(local_0: bool, local_1: int32, local_2: int32) -> int32 {
  if local_0 {
    local_1
  } else {
    local_2
  }
}

fn main() -> () {
  let local_0 = choose(true, 10, 99) in
  let local_1 = choose(false, 10, 99) in
  let _ = string_println("yes=" + int32_to_string(local_0)) in
  let _ = string_println("no=" + int32_to_string(local_1)) in
  ()
}