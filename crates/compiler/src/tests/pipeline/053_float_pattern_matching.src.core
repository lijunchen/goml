fn summarize(event/0: FloatEvent) -> string {
  match event/0 {
    FloatEvent::Sample32(x0, x1) => {
      let x0 = FloatEvent::Sample32._0(event/0) in
      let x1 = FloatEvent::Sample32._1(event/0) in
      let value/2 = x1 in
      let label/1 = x0 in
      string_add(label/1, float32_to_string(value/2))
    },
    FloatEvent::Sample64(x2, x3) => {
      let x2 = FloatEvent::Sample64._0(event/0) in
      let x3 = FloatEvent::Sample64._1(event/0) in
      let value/4 = x3 in
      let label/3 = x2 in
      string_add(label/3, float64_to_string(value/4))
    },
  }
}

fn compare(values/5: (float32, float64)) -> string {
