fn fib(x/0: int32) -> int32 {
  let mtmp0 = int32_less(x/0, 2) in
  match mtmp0 {
    true => {
      1
    },
    false => {
      let t2 = int32_sub(x/0, 1) in
      let t1 = fib(t2) in
      let t4 = int32_sub(x/0, 2) in
      let t3 = fib(t4) in
      int32_add(t1, t3)
    },
  }
}

fn main() -> unit {
  let t6 = fib(10) in
  let t5 = int32_to_string(t6) in
  string_print(t5)
}