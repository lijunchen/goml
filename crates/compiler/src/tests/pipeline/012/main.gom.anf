fn fib(x/0: int32) -> int32 {
  let mtmp0 = (x/0 < 2) in
  match mtmp0 {
    true => {
      1
    },
    false => {
      let t2 = (x/0 - 1) in
      let t1 = fib(t2) in
      let t4 = (x/0 - 2) in
      let t3 = fib(t4) in
      (t1 + t3)
    },
  }
}

fn main() -> unit {
  let t6 = fib(10) in
  let t5 = int32_to_string(t6) in
  string_print(t5)
}