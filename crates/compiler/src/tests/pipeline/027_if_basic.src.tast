fn choose(flag/0: bool, x/1: int, y/2: int) -> int {
  if (flag/0 : bool) {
      (x/1 : int)
  } else {
      (y/2 : int)
  }
}

fn main() -> unit {
  let yes/3: int = (choose : (bool, int, int) -> int)(true, 10, 99) in
  let no/4: int = (choose : (bool, int, int) -> int)(false, 10, 99) in
  let _ : unit = (string_println : (string) -> unit)((string_add : (string, string) -> string)("yes=", (int_to_string : (int) -> string)((yes/3 : int)))) in
  let _ : unit = (string_println : (string) -> unit)((string_add : (string, string) -> string)("no=", (int_to_string : (int) -> string)((no/4 : int)))) in
  ()
}