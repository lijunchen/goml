fn choose(flag/0: bool, x/1: int, y/2: int) -> int {
  if (flag/0 : bool) {
      (x/1 : int)
  } else {
      (y/2 : int)
  }
}

fn main() -> unit {
  let yes/3: int = choose(true, 10, 99) in
  let no/4: int = choose(false, 10, 99) in
  let _ : unit = string_println(string_add("yes=", int_to_string((yes/3 : int)))) in
  let _ : unit = string_println(string_add("no=", int_to_string((no/4 : int)))) in
  ()
}