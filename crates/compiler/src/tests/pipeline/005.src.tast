fn main() -> unit {
  let a/0: (Color, Color) = (Color::Blue, Color::Red) in
  match (a/0 : (Color, Color)) {
      (Color::Red, Color::Green) => string_print(int_to_string(0)),
      (Color::Red, Color::Red) => string_print(int_to_string(1)),
      (Color::Blue, Color::Blue) => string_print(int_to_string(2)),
      _ : (Color, Color) => string_print(int_to_string(3)),
  }
}