fn main() -> unit {
    // Test Vec with int32
    let vi: Vec[int32] = vec_new();
    let vi = vec_push(vi, 42);
    let val_i = vec_get(vi, 0);
    let len_i = vec_len(vi);
    
    // Test Vec with string
    let vs: Vec[string] = vec_new();
    let vs = vec_push(vs, "hello");
    let vs = vec_push(vs, "world");
    let val_s = vec_get(vs, 1);
    let len_s = vec_len(vs);
    
    // Test Vec with bool
    let vb: Vec[bool] = vec_new();
    let vb = vec_push(vb, true);
    let vb = vec_push(vb, false);
    let val_b = vec_get(vb, 0);
    let len_b = vec_len(vb);
    
    // Print results
    let _ = string_println(int32_to_string(val_i));
    let _ = string_println(int32_to_string(len_i));
    let _ = string_println(val_s);
    let _ = string_println(int32_to_string(len_s));
    let _ = string_println(bool_to_string(val_b));
    let _ = string_println(int32_to_string(len_b));
    ()
}