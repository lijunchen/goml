fn test_int_comparisons() -> unit {
    let a/0: int32 = 10;
    let b/1: int32 = 20;
    let c/2: int32 = 10;
    let less/3: bool = (a/0 : int32) < (b/1 : int32);
    let _ : unit = (string_println : (string) -> unit)("10 < 20: " + (bool_to_string : (bool) -> string)((less/3 : bool)));
    let greater/4: bool = (b/1 : int32) > (a/0 : int32);
    let _ : unit = (string_println : (string) -> unit)("20 > 10: " + (bool_to_string : (bool) -> string)((greater/4 : bool)));
    let less_eq1/5: bool = (a/0 : int32) <= (b/1 : int32);
    let _ : unit = (string_println : (string) -> unit)("10 <= 20: " + (bool_to_string : (bool) -> string)((less_eq1/5 : bool)));
    let less_eq2/6: bool = (a/0 : int32) <= (c/2 : int32);
    let _ : unit = (string_println : (string) -> unit)("10 <= 10: " + (bool_to_string : (bool) -> string)((less_eq2/6 : bool)));
    let greater_eq1/7: bool = (b/1 : int32) >= (a/0 : int32);
    let _ : unit = (string_println : (string) -> unit)("20 >= 10: " + (bool_to_string : (bool) -> string)((greater_eq1/7 : bool)));
    let greater_eq2/8: bool = (c/2 : int32) >= (a/0 : int32);
    let _ : unit = (string_println : (string) -> unit)("10 >= 10: " + (bool_to_string : (bool) -> string)((greater_eq2/8 : bool)));
    let eq1/9: bool = (a/0 : int32) == (c/2 : int32);
    let _ : unit = (string_println : (string) -> unit)("10 == 10: " + (bool_to_string : (bool) -> string)((eq1/9 : bool)));
    let eq2/10: bool = (a/0 : int32) == (b/1 : int32);
    let _ : unit = (string_println : (string) -> unit)("10 == 20: " + (bool_to_string : (bool) -> string)((eq2/10 : bool)));
    let neq1/11: bool = (a/0 : int32) != (b/1 : int32);
    let _ : unit = (string_println : (string) -> unit)("10 != 20: " + (bool_to_string : (bool) -> string)((neq1/11 : bool)));
    let neq2/12: bool = (a/0 : int32) != (c/2 : int32);
    let _ : unit = (string_println : (string) -> unit)("10 != 10: " + (bool_to_string : (bool) -> string)((neq2/12 : bool)));
    ();
}

fn test_float_comparisons() -> unit {
    let x/13: float64 = 3.14;
    let y/14: float64 = 2.71;
    let z/15: float64 = 3.14;
    let less/16: bool = (y/14 : float64) < (x/13 : float64);
    let _ : unit = (string_println : (string) -> unit)("2.71 < 3.14: " + (bool_to_string : (bool) -> string)((less/16 : bool)));
    let greater/17: bool = (x/13 : float64) > (y/14 : float64);
    let _ : unit = (string_println : (string) -> unit)("3.14 > 2.71: " + (bool_to_string : (bool) -> string)((greater/17 : bool)));
    let less_eq1/18: bool = (y/14 : float64) <= (x/13 : float64);
    let _ : unit = (string_println : (string) -> unit)("2.71 <= 3.14: " + (bool_to_string : (bool) -> string)((less_eq1/18 : bool)));
    let less_eq2/19: bool = (x/13 : float64) <= (z/15 : float64);
    let _ : unit = (string_println : (string) -> unit)("3.14 <= 3.14: " + (bool_to_string : (bool) -> string)((less_eq2/19 : bool)));
    let greater_eq1/20: bool = (x/13 : float64) >= (y/14 : float64);
    let _ : unit = (string_println : (string) -> unit)("3.14 >= 2.71: " + (bool_to_string : (bool) -> string)((greater_eq1/20 : bool)));
    let greater_eq2/21: bool = (z/15 : float64) >= (x/13 : float64);
    let _ : unit = (string_println : (string) -> unit)("3.14 >= 3.14: " + (bool_to_string : (bool) -> string)((greater_eq2/21 : bool)));
    let eq1/22: bool = (x/13 : float64) == (z/15 : float64);
    let _ : unit = (string_println : (string) -> unit)("3.14 == 3.14: " + (bool_to_string : (bool) -> string)((eq1/22 : bool)));
    let eq2/23: bool = (x/13 : float64) == (y/14 : float64);
    let _ : unit = (string_println : (string) -> unit)("3.14 == 2.71: " + (bool_to_string : (bool) -> string)((eq2/23 : bool)));
    let neq1/24: bool = (x/13 : float64) != (y/14 : float64);
    let _ : unit = (string_println : (string) -> unit)("3.14 != 2.71: " + (bool_to_string : (bool) -> string)((neq1/24 : bool)));
    let neq2/25: bool = (x/13 : float64) != (z/15 : float64);
    let _ : unit = (string_println : (string) -> unit)("3.14 != 3.14: " + (bool_to_string : (bool) -> string)((neq2/25 : bool)));
    ();
}

fn main() -> unit {
    let _ : unit = (string_println : (string) -> unit)("=== Integer Comparisons ===");
    let _ : unit = (test_int_comparisons : () -> unit)();
    let _ : unit = (string_println : (string) -> unit)("");
    let _ : unit = (string_println : (string) -> unit)("=== Float Comparisons ===");
    let _ : unit = (test_float_comparisons : () -> unit)();
    ();
}