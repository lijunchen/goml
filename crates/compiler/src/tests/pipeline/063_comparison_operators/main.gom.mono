  let eq1/9 = (a/0 == c/2) in
  let mtmp6 = string_println(("10 == 10: " + bool_to_string(eq1/9))) in
  let eq2/10 = (a/0 == b/1) in
  let mtmp7 = string_println(("10 == 20: " + bool_to_string(eq2/10))) in
  let neq1/11 = (a/0 != b/1) in
  let mtmp8 = string_println(("10 != 20: " + bool_to_string(neq1/11))) in
  let neq2/12 = (a/0 != c/2) in
  let mtmp9 = string_println(("10 != 10: " + bool_to_string(neq2/12))) in
  ()
}

fn test_float_comparisons() -> unit {
  let x/13 = 3.14 in
  let y/14 = 2.71 in
  let z/15 = 3.14 in
  let less/16 = (y/14 < x/13) in
  let mtmp10 = string_println(("2.71 < 3.14: " + bool_to_string(less/16))) in
  let greater/17 = (x/13 > y/14) in
  let mtmp11 = string_println(("3.14 > 2.71: " + bool_to_string(greater/17))) in
  let less_eq1/18 = (y/14 <= x/13) in
  let mtmp12 = string_println(("2.71 <= 3.14: " + bool_to_string(less_eq1/18))) in
  let less_eq2/19 = (x/13 <= z/15) in
  let mtmp13 = string_println(("3.14 <= 3.14: " + bool_to_string(less_eq2/19))) in
  let greater_eq1/20 = (x/13 >= y/14) in
  let mtmp14 = string_println(("3.14 >= 2.71: " + bool_to_string(greater_eq1/20))) in
  let greater_eq2/21 = (z/15 >= x/13) in
  let mtmp15 = string_println(("3.14 >= 3.14: " + bool_to_string(greater_eq2/21))) in
  let eq1/22 = (x/13 == z/15) in
  let mtmp16 = string_println(("3.14 == 3.14: " + bool_to_string(eq1/22))) in
  let eq2/23 = (x/13 == y/14) in
  let mtmp17 = string_println(("3.14 == 2.71: " + bool_to_string(eq2/23))) in
  let neq1/24 = (x/13 != y/14) in
  let mtmp18 = string_println(("3.14 != 2.71: " + bool_to_string(neq1/24))) in
  let neq2/25 = (x/13 != z/15) in
  let mtmp19 = string_println(("3.14 != 3.14: " + bool_to_string(neq2/25))) in
  ()
}

