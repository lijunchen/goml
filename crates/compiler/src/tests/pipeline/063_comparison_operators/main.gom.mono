fn test_int_comparisons() -> unit {
  let a/0 = 10 in
  let b/1 = 20 in
  let c/2 = 10 in
  let less/3 = (a/0 < b/1) in
  let mtmp0 = string_println(("10 < 20: " + bool_to_string(less/3))) in
  let greater/4 = (b/1 > a/0) in
  let mtmp1 = string_println(("20 > 10: " + bool_to_string(greater/4))) in
  let less_eq1/5 = (a/0 <= b/1) in
  let mtmp2 = string_println(("10 <= 20: " + bool_to_string(less_eq1/5))) in
  let less_eq2/6 = (a/0 <= c/2) in
  let mtmp3 = string_println(("10 <= 10: " + bool_to_string(less_eq2/6))) in
  let greater_eq1/7 = (b/1 >= a/0) in
  let mtmp4 = string_println(("20 >= 10: " + bool_to_string(greater_eq1/7))) in
  let greater_eq2/8 = (c/2 >= a/0) in
