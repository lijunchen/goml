fn main() {
  let start8: uint8 = 200;
  let add8: uint8 = 55;
  let sum8 = start8 + add8;
  let neg8 = -start8;

  let start16: uint16 = 50000;
  let add16: uint16 = 12000;
  let sum16 = start16 + add16;
  let diff16 = sum16 - start16;

  let start32: uint32 = 3000000000;
  let add32: uint32 = 123456789;
  let sum32 = start32 + add32;
  let neg32 = -add32;

  let start64: uint64 = 6000000000;
  let add64: uint64 = 4000000000;
  let sum64 = start64 + add64;
  let diff64 = sum64 - add64;

  let message = uint8_to_string(sum8)
    + ", " + uint8_to_string(neg8)
    + "; " + uint16_to_string(diff16)
    + "; " + uint32_to_string(neg32)
    + "; " + uint64_to_string(diff64);
  string_println(message);
}
