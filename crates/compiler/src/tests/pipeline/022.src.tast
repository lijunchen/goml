fn match_string(s/0: string) -> int {
  match (s/0 : string) {
      "hello" => 1,
      "world" => 2,
      _ : string => 3,
  }
}

fn wildcard_position(s/1: string) -> int {
  match (s/1 : string) {
      _ : string => 4,
      "world" => 5,
  }
}

fn repeated_string(s/2: string) -> int {
  match (s/2 : string) {
      "hello" => 6,
      "hello" => 7,
      _ : string => 8,
  }
}

