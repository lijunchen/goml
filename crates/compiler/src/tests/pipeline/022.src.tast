  let _ : unit = (string_println : (string) -> unit)((int_to_string : (int) -> string)((match_string : (string) -> int)("hello"))) in
  let _ : unit = (string_println : (string) -> unit)((int_to_string : (int) -> string)((match_string : (string) -> int)("planet"))) in
  let _ : unit = (string_println : (string) -> unit)((int_to_string : (int) -> string)((wildcard_position : (string) -> int)("world"))) in
  let _ : unit = (string_println : (string) -> unit)((int_to_string : (int) -> string)((wildcard_position : (string) -> int)("sun"))) in
  let _ : unit = (string_println : (string) -> unit)((int_to_string : (int) -> string)((repeated_string : (string) -> int)("hello"))) in
  (string_println : (string) -> unit)((int_to_string : (int) -> string)((repeated_string : (string) -> int)("mars")))
