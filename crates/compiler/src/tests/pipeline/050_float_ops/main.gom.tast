fn lerp32(a/6: float32, b/7: float32, weight/8: float32) -> float32 {
    let delta/9: float32 = (b/7 : float32) - (a/6 : float32);
    (a/6 : float32) + (delta/9 : float32) * (weight/8 : float32);
}

fn midpoint_energy(x/10: float64, y/11: float64) -> float64 {
    let sum/12: float64 = (x/10 : float64) * (x/10 : float64) + (y/11 : float64) * (y/11 : float64);
    (sum/12 : float64) / 2;
}

fn main() -> unit {
    let start32/13: float32 = 1.25;
    let end32/14: float32 = 5.75;
    let half/15: float32 = 0.5;
    let scale/16: float32 = 2;
    let mid32/17: float32 = (lerp32 : (float32, float32, float32) -> float32)((start32/13 : float32), (end32/14 : float32), (half/15 : float32));
    let neg_end32/18: float32 = -(end32/14 : float32);
    let ratio32/19: float32 = (end32/14 : float32) / (scale/16 : float32);
    let less32/20: bool = (start32/13 : float32) < (end32/14 : float32);
    let dx/21: float64 = 6.5;
    let dy/22: float64 = 3.5;
    let quarter/23: float64 = 0.25;
    let energy/24: float64 = (midpoint_energy : (float64, float64) -> float64)((dx/21 : float64), (dy/22 : float64));
    let neg_dx/25: float64 = -(dx/21 : float64);
    let adjusted/26: float64 = (energy/24 : float64) + (dy/22 : float64) - (dx/21 : float64) * (quarter/23 : float64);
    let threshold/27: float64 = 4;
    let less64/28: bool = (adjusted/26 : float64) < (threshold/27 : float64);
    (show32 : (string, float32) -> unit)("mid32=", (mid32/17 : float32));
    (show32 : (string, float32) -> unit)("neg_end32=", (neg_end32/18 : float32));
    (show32 : (string, float32) -> unit)("ratio32=", (ratio32/19 : float32));
    let _ : unit = (string_println : (string) -> unit)("less32=" + (bool_to_string : (bool) -> string)((less32/20 : bool)));
    (show64 : (string, float64) -> unit)("energy=", (energy/24 : float64));
    (show64 : (string, float64) -> unit)("neg_dx=", (neg_dx/25 : float64));
    (show64 : (string, float64) -> unit)("adjusted=", (adjusted/26 : float64));
    let _ : unit = (string_println : (string) -> unit)("less64=" + (bool_to_string : (bool) -> string)((less64/28 : bool)));
    ();
}