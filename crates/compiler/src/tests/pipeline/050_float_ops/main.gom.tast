    let _ : unit = (string_println : (string) -> unit)((message/2 : string));
    ();
}

fn show64(label/3: string, value/4: float64) -> unit {
    let message/5: string = (label/3 : string) + (float64_to_string : (float64) -> string)((value/4 : float64));
    let _ : unit = (string_println : (string) -> unit)((message/5 : string));
