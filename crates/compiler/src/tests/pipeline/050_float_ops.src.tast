fn lerp32(a/6: float32, b/7: float32, weight/8: float32) -> float32 {
  let delta/9: float32 = (b/7 : float32) - (a/6 : float32) in
  (a/6 : float32) + (delta/9 : float32) * (weight/8 : float32)
}

fn midpoint_energy(x/10: float64, y/11: float64) -> float64 {
  let sum/12: float64 = (x/10 : float64) * (x/10 : float64) + (y/11 : float64) * (y/11 : float64) in
  (sum/12 : float64) / 2
}

