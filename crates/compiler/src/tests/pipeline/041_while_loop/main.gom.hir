fn sum_to(local_0: int32) -> int32 {
  let local_1 = ref(0) in
  let local_2 = ref(0) in
  let _ = while ref_get(local_2) < local_0 {
    let local_3 = ref_get(local_2) in
    let _ = ref_set(local_1, ref_get(local_1) + local_3) in
    let _ = ref_set(local_2, local_3 + 1) in
    ()
  } in
  ref_get(local_1)
}

fn sum_even(local_0: int32) -> int32 {
  let local_1 = ref(0) in
  let local_2 = ref(0) in
  let local_3 = ref(true) in
  let _ = while ref_get(local_2) < local_0 {
    let local_4 = ref_get(local_2) in
    let _ = ref_set(local_2, local_4 + 1) in
    let local_5 = ref_get(local_3) in
    let _ = ref_set(local_3, !local_5) in
    if local_5 {
      ref_set(local_1, ref_get(local_1) + local_4)
    } else {
      ()
    }
  } in
  ref_get(local_1)
}

fn main() -> () {
  let local_0 = sum_to(5) in
  let local_1 = sum_even(6) in
  let _ = string_println("sum_to(5)=" + int32_to_string(local_0)) in
  let _ = string_println("sum_even(6)=" + int32_to_string(local_1)) in
  ()
}