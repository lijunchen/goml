fn main() {
    let start8: uint8 = 200 in
    let add8: uint8 = 55 in
    let sum8 = start8 + add8 in
    let neg8 = -start8 in
    let start16: uint16 = 50000 in
    let add16: uint16 = 12000 in
    let sum16 = start16 + add16 in
    let diff16 = sum16 - start16 in
    let start32: uint32 = 3000000000 in
    let add32: uint32 = 123456789 in
    let sum32 = start32 + add32 in
    let neg32 = -add32 in
    let start64: uint64 = 6000000000 in
    let add64: uint64 = 4000000000 in
    let sum64 = start64 + add64 in
    let diff64 = sum64 - add64 in
    let message = uint8_to_string(sum8) + ", " + uint8_to_string(neg8) + "; " + uint16_to_string(diff16) + "; " + uint32_to_string(neg32) + "; " + uint64_to_string(diff64) in
    let _ = string_println(message) in
    ()
}

