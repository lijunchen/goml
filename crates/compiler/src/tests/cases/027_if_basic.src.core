fn choose(flag/0: bool, x/1: int, y/2: int) -> int {
  if flag/0 {
      x/1
  } else {
      y/2
  }
}

fn main() -> unit {
  let yes/3 = choose(true, 10, 99) in
  let no/4 = choose(false, 10, 99) in
  let mtmp0 = string_println(string_add("yes=", int_to_string(yes/3))) in
  let mtmp1 = string_println(string_add("no=", int_to_string(no/4))) in
  ()
}