fn fib(x/0: Int) -> Int {
  let mtmp0 = int_less(x/0, 2) in
  match mtmp0 {
    true => {
      1
    },
    false => {
      let t2 = int_sub(x/0, 1) in
      let t1 = fib(t2) in
      let t4 = int_sub(x/0, 2) in
      let t3 = fib(t4) in
      int_add(t1, t3)
    },
  }
}

fn main() -> Unit {
  let t6 = fib(10) in
  let t5 = int_to_string(t6) in
  string_print(t5)
}