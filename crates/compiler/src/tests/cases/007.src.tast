main () :Unit let a/0 = Expr::Mul(Expr::Add(Expr::Zero,Expr::Zero),Expr::Zero);
match (a/0 : Expr) {
    Expr::Add(Expr::Zero,Expr::Zero) => print_int( 0 ),
    Expr::Mul(Expr::Zero,x/1) => print_int( 1 ),
    Expr::Add(Expr::Succ(x/2),y/3) => print_int( 2 ),
    Expr::Mul(x/4,Expr::Zero) => print_int( 3 ),
    Expr::Mul(Expr::Add(x/5,y/6),z/7) => print_int( 4 ),
    Expr::Add(x/8,Expr::Zero) => print_int( 5 ),
    x/9 => print_int( 6 ),
}