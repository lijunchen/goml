test (t/0:T) :Unit match (t/0 : T) {
    T::A => print_int( 1 ),
    T::B(false,false) => print_int( 2 ),
    T::B(false,true) => print_int( 3 ),
    _ : T => print_int( 4 ),
}
main () :Unit let _ : (T) -> Unit = test( T::B(true,true) );
let _ : (T) -> Unit = test( T::B(false,true) );
let _ : (T) -> Unit = test( T::B(false,false) );
let _ : (T) -> Unit = test( T::A );
()