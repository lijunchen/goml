fn impl_Display_Point_show(self/0: Point) -> string {
  "Point"
}

fn impl_Display_Option_int_show(self/1: Option[int]) -> string {
  match self/1 {
    Option::Some(x0) => {
      let x0 = Option::Some._0(self/1) in
      let value/2 = x0 in
      string_add("Some(", string_add(int_to_string(value/2), ")"))
    },
    Option::None => {
      "None"
    },
  }
}

fn make_optional(flag/3: bool) -> Option[int] {
  if flag/3 {
      Option::Some(42)
  } else {
      Option::None
  }
}

fn main() -> unit {
  let point/4 = Point { x: 1, y: 2 } in
  let some_number/5 = make_optional(true) in
  let none_number/6 = make_optional(false) in
  let mtmp1 = string_println(impl_Display_Point_show(point/4)) in
  let mtmp2 = string_println(impl_Display_Option_int_show(some_number/5)) in
  let mtmp3 = string_println(impl_Display_Option_int_show(none_number/6)) in
  ()
}