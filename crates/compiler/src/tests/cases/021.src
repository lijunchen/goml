fn match_int(n: int) -> int {
  match n {
    0 => 10,
    1 => 20,
    _ => 30,
  }
}

fn wildcard_first(n: int) -> int {
  match n {
    _ => 40,
    0 => 50,
  }
}

fn wildcard_middle(n: int) -> int {
  match n {
    2 => 90,
    _ => 100,
    3 => 110,
  }
}

fn repeated(n: int) -> int {
  match n {
    1 => 60,
    1 => 70,
    _ => 80,
  }
}

fn main() {
    let _ = string_println(int_to_string(match_int(0))) in
    let _ = string_println(int_to_string(match_int(5))) in
    let _ = string_println(int_to_string(wildcard_first(0))) in
    let _ = string_println(int_to_string(wildcard_first(2))) in
    let _ = string_println(int_to_string(wildcard_middle(2))) in
    let _ = string_println(int_to_string(wildcard_middle(3))) in
    let _ = string_println(int_to_string(repeated(1))) in
    string_println(int_to_string(repeated(3)))
}
