fn update_array(arr: [int; 3]) -> [int; 3] {
    array_set(arr, 1, 42)
}

fn read_array(arr: [int; 3]) -> int {
    array_get(arr, 1)
}

fn main() {
    let arr = [1, 2, 3] in
    let updated = update_array(arr) in
    let value = read_array(updated) in
    let _ = string_println(int_to_string(value)) in
    ()
}
