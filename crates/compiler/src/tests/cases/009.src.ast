enum T {
    A,
    B(Bool, Bool)
}

fn test(t:T) -> Unit {
    match t {
        A => print(int_to_string(1)),
        B(false, false) => print(int_to_string(2)),
        B(false, true) => print(int_to_string(3)),
        _ => print(int_to_string(4)),
    }
}

fn main() {
    let _ = test(B(true, true)) in
    let _ = test(B(false, true)) in
    let _ = test(B(false, false)) in
    let _ = test(A) in
    ()
}

