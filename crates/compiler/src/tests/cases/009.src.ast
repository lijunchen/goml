enum T {
    A,
    B(Bool, Bool)
}

fn test(t:T) -> Unit {
    match t {
        A => print_int(1),
        B(false, false) => print_int(2),
        B(false, true) => print_int(3),
        _ => print_int(4),
    }
}

fn main() {
    let _ = test(B(true, true)) in
    let _ = test(B(false, true)) in
    let _ = test(B(false, false)) in
    let _ = test(A) in
    ()
}

