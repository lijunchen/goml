fn test(t/0:T) -> Unit {
  match t/0 {
    T::A => {
      print_int(1)
    },
    T::B(x0,x1) => {
      let x1 = T_1_1(t/0) in
      let x0 = T_1_0(t/0) in
      match x1 {
        true => {
          match x0 {
            true => {
              print_int(4)
            },
            false => {
              print_int(3)
            },
          }
        },
        false => {
          match x0 {
            true => {
              print_int(4)
            },
            false => {
              print_int(2)
            },
          }
        },
      }
    },
  }
}

fn main() -> Unit {
